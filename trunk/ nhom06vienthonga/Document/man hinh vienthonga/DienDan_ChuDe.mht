}

.blockhead .popupctrl:hover {
	color: #ffffff;
}
.popupbody {
	background:#e1e1e1 url(images/misc/blog/top-highlight.png) repeat-x  ;
	border:1px solid #c4c4c4; =

	padding:2px;
	position:absolute;
	z-index:1000;
	top:1.5em;
	left:0;
	display:none;
	min-width:200px;
}

.popupmenu:hover ul.popuphover {
	display:block;
}

.popupbody li {
	border-top:solid 1px #e1e1e1;
}

.popupbody > li:first-child {
	border-top:none;
}

.popupbody li > a,
.popupbody li > label {
	display:block;
